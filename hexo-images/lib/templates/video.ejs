<figure>
    <video autoplay="true" loop="true" muted="true">
        <%_ for (const type of Object.keys(files).filter((f) => f !== 'default')) { _%>
            <source src="<%= files[type] %>" type="video/<%= type %>">
        <%_ } _%>
    </video>
    <%_ if (caption) { _%>
        <figcaption><%= caption %></figcaption>
    <%_ } _%>
</figure>
