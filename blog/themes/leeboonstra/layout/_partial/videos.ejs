<div class="container videos">
    <div class="row justify-content-md-center">
      <div class="col">
        <h1><%-page.title-%></h1>
  
        <div class="card w-100" >
          <div class="shadow-sm">
            <div class="embed-responsive embed-responsive-16by9">
              <iframe id="youtubeFrame" class="embed-responsive-item" src="https://www.youtube.com/embed/Ob_Oc3U1vS4" allowfullscreen></iframe>
            </div>
            <div class="card-body">
              <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
            </div>
          </div>
        </div>

        <% site.categories.findOne({name: 'Videos'}).posts.forEach(function(post) { %>
          <div class="card w-25 p-2 float-md-left youtube">
            <div class="shadow-sm">
              <img class="card-img-top" src="https://img.youtube.com/vi/<%-post.youtube-%>/0.jpg" alt="Watch on YouTube">
              <div class="card-body">
                <p class="card-text"><%-post.title-%></p>
              </div>
              <div class="foot">
                <a href="#" class="btn youtubeLink btn-primary stretched-link">
                  <span class="hidden youtubeId"><%-post.youtube-%></span>
                  Watch
              </a>
              </div>
            </div>
          </div>
        <% }) %>
    
      </div>
    </div>
  </div>