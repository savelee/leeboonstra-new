  <meta name="mobile-web-app-capable" content="yes" />
  <meta name="HandheldFriendly" content="True"/>
  <meta name="MobileOptimized" content="640"/>
  
  <%- autoCanonical(config, page) %>

  <meta name="theme-color" content="#fafafa"/>

