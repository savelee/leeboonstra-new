<head>
  <meta charset="utf-8">

  <%
  var title = page.title;
  if (is_archive()){
    title = 'Archive';
    if (is_month()){
      title += ': ' + page.year + '/' + page.month;
    } else if (is_year()){
      title += ': ' + page.year;
    }
  } else if (is_category()){
    title = 'Conversational AI: ' + page.category;
  } else if (is_tag()){
    title = page.tag;
  }

  // keywords.
  var keyWords = config.keywords ? config.keywords : config.title;
  %>
  <title><% if (title) { %><%= title %> <% } else { %><%= config.title %> <% } %></title>
  <link rel="alternate" href="/atom.xml" title="<% if (title) { %><%= title %> <% } else { %><%= config.title %> <% } %>" type="application/atom+xml"/>
  
  <meta name="description" content="<% if (page.description) { %><%= page.description %> <% } else { %><%= config.description %> <% } %>"/>
  
  <meta name="viewport" content="width=device-width,initial-scale=1">
