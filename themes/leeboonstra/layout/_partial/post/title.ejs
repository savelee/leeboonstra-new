<% if (post.link){ %>
  <% if (post.featured){ %>
    <h1 class="hidden">Blog:Learn Machine Learning from a Google Artificial Intelligence Engineer</h1>
    <h2 itemprop="headline" class="h1">
      <a class="<%= class_name %>" href="<%- url_for(post.link) %>" target="_blank" itemprop="url"><%- post.title %></a>
    </h2>
  <% } else { %>
    <h2><a class="<%= class_name %>" href="<%- url_for(post.link) %>" target="_blank" itemprop="url"><%- truncate(post.title, {length: 40, omission: '...'}) %></a></h2>
   <% } %>
<% } else if (post.title){ %>
  <% if (index){ %>
    <% if (highlight){ %>
    <h1 class="hidden">Blog: Learn Machine Learning from a Google AI Engineer</h1>
    <h2 itemprop="headline" class="h1">
      <a class="<%= class_name %>" href="<%- url_for(post.path) %>">
          <%- post.title %>
      </a>
    </h1>
    <% } else { %>
      <h2><a class="<%= class_name %> h1" href="<%- url_for(post.link) %>" target="_blank" itemprop="url"><%- truncate(post.title, {length: 40, omission: '...'}) %></a></h2>
    <% } %>
  <% } else { %>
    <h1 class="<%= class_name %>" itemprop="headline">
      <%- truncate(post.title, {length: 45, omission: '...'}) %>
    </h1>
  <% } %>
<% } %>